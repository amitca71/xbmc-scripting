<window>

<!--
	The <resolution> tag is used for the setCoordinateResolution() function.
	
	You may move, add or delete any controls you want as long as they're not Required.
		
	<type>list</type> have an additional tag <hidespinner>, if set to true, a scrollbar will be used
	in place of the spinner control.
	
	<include> tags work and the defaults.xml file will be used for any missing tags.
	id's  2,3,4,12,50-59 are reserved
	
	all skin elements with id="1" are purely cosmetic and unreferenced by the python code 
	go nuts with them
-->

	<defaultcontrol always="true">11</defaultcontrol>
	<resolution>PAL</resolution>
    <useincludes>false</useincludes>

	<controls>

		<control type="image" id="1">
			<description>pause image</description>
			<posx>315</posx>
			<posy>170</posy>
			<width>213</width>
			<height>113</height>
			<texture>\pause.png</texture>
		</control>
		
		<control type="label" id="1">
			<description>"High Score:" label</description>
			<posx>330</posx>
			<posy>100</posy>
			<width>150</width>
			<height>25</height>
			<label>10</label>
			<font>font14</font> 
			<textcolor>FFFFFF00</textcolor> 
		</control>
