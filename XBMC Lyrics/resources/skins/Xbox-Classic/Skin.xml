<window id="2006">
    <allowoverlay>no</allowoverlay>
    <defaultcontrol always="false">4</defaultcontrol>
    <resolution>PAL</resolution>
    <coordinates>
        <system>1</system>
        <posx>0</posx>
        <posy>0</posy>
    </coordinates>

    <controls>

        <control type="image" id="1">
            <description>background image</description>
            <posx>188</posx>
            <posy>30</posy>
            <width>295</width>
            <height>180</height>
            <texture>dialog_top.png</texture>
            <diffusecolor>DAFFFFFF</diffusecolor>
        </control>

        <control type="image" id="1">
            <description>background image</description>
            <posx>228</posx>
            <posy>70</posy>
            <width>440</width>
            <height>442</height>
            <texture>dialog_middle.png</texture>
            <colordiffuse>BBFFFFFF</colordiffuse>
        </control>

        <control type="image" id="1">
            <description>divider image</description>
            <posx>248</posx>
            <posy>182</posy>
            <width>400</width>
            <height>4</height>
            <texture>calibrate-subtitles-focus.png</texture>
            <colordiffuse>BBFFFFFF</colordiffuse>
        </control>
        
        <control type="image" id="1">
            <description>background image</description>
            <posx>410</posx>
            <posy>390</posy>
            <width>290</width>
            <height>160</height>
            <texture>dialog_bottom.png</texture>
            <diffusecolor>DAFFFFFF</diffusecolor>
        </control>

        <control type="textbox" id="3">
            <description>textarea</description>
            <posx>240</posx>
            <posy>195</posy>
            <width>415</width>
            <height>320</height>
            <onup>3</onup>
            <ondown>3</ondown>
            <onleft>3</onleft>
            <onright>3</onright>
            <spinposx>410</spinposx>
            <spinposy>345</spinposy>
            <spincolor>ffffffff</spincolor>
            <textureup>scroll-up.png</textureup>
            <texturedown>scroll-down.png</texturedown>
            <textureupfocus>scroll-up-focus.gif</textureupfocus>
            <texturedownfocus>scroll-down-focus.gif</texturedownfocus>
            <font>font13</font>
            <textcolor>DDFFFFFF</textcolor>
            <visible>false</visible>
        </control>

        <control type="list" id="4">
            <description>lyrics list</description>
            <posx>240</posx>
            <posy>202</posy>
            <width>416</width>
            <height>299</height>
            <onleft>4</onleft>
            <onright>4</onright>
            <onup>4</onup>
            <ondown>4</ondown>
            <spinposx>410</spinposx>
            <spinposy>345</spinposy>
            <spincolor>ddffffff</spincolor>
            <textureup>scroll-up.png</textureup>
            <texturedown>scroll-down.png</texturedown>
            <textureupfocus>scroll-up-focus.gif</textureupfocus>
            <texturedownfocus>scroll-down-focus.gif</texturedownfocus>
            <texturefocus>list-focus.png</texturefocus>
            <texturenofocus>-</texturenofocus>
            <image>-</image>
			<itemwidth>0</itemwidth>
			<itemheight>0</itemheight>
            <textureheight>22</textureheight>
            <spacebetweenitems>1</spacebetweenitems>  
            <font>font13</font>
            <selectedcolor>ffffffff</selectedcolor>
            <textcolor>DDFFFFFF</textcolor>
            <textcolor2>ddffffff</textcolor2>
            <colordiffuse>ffffffff</colordiffuse>
            <suffix> |</suffix>
            <textxoff>3</textxoff>
            <textxoff2>0</textxoff2>
            <aligny>center</aligny>
            <visible>false</visible>
			<textxoff>0</textxoff>
        </control>

        <control type="list" id="5">
            <description>link list</description>
            <posx>240</posx>
            <posy>202</posy>
            <width>416</width>
            <height>310</height>
            <onleft>5</onleft>
            <onright>5</onright>
            <onup>5</onup>
            <ondown>5</ondown>
            <spinposx>410</spinposx>
            <spinposy>345</spinposy>
            <spincolor>ddffffff</spincolor>
            <textureup>scroll-up.png</textureup>
            <texturedown>scroll-down.png</texturedown>
            <textureupfocus>scroll-up-focus.gif</textureupfocus>
            <texturedownfocus>scroll-down-focus.gif</texturedownfocus>
            <texturefocus>list-focus.png</texturefocus>
            <texturenofocus>list-nofocus.png</texturenofocus>
            <image>-</image>
			<itemwidth>0</itemwidth>
			<itemheight>0</itemheight>
            <textureheight>29</textureheight>
            <spacebetweenitems>2</spacebetweenitems>  
			<textxoff>0</textxoff>
            <font>font13</font>
            <selectedcolor>ffffffff</selectedcolor>
            <textcolor>DDFFFFFF</textcolor>
            <textcolor2>ddffffff</textcolor2>
            <colordiffuse>ffffffff</colordiffuse>
            <suffix> |</suffix>
            <textxoff>3</textxoff>
            <textxoff2>0</textxoff2>
            <aligny>center</aligny>
            <visible>false</visible>
        </control>

        <control type="label" id="7">
            <description>list label</description>
            <posx>647</posx>
            <posy>481</posy>
            <width>100</width>
            <label>1/4</label>
            <align>right</align>
            <font>font13</font>
            <textcolor>DDFFFFFF</textcolor>
            <visible>false</visible>
        </control>

        <control type="group">
            <visible>Player.ShowInfo</visible>
            <animation effect="fade" time="1000">VisibleChange</animation>
            <control type="group">
                <posx>40</posx>
                <posy>350</posy>
                <control type="image">
                    <description>Dialog Header</description>
                    <posx>40</posx>
                    <posy>350</posy>
                    <width>150</width>
                    <height>100</height>
                    <texture>dialog_top.png</texture>
                </control>
                <control type="image">
                    <description>Dialog Text Area</description>
                    <posx>60</posx>
                    <posy>372</posy>
                    <width>150</width>
                    <height>150</height>
                    <colordiffuse>BBFFFFFF</colordiffuse>
                    <texture border="10">dialog_middle.png</texture>
                </control>
                <control type="image">
                    <description>Dialog Footer</description>
                    <posx>76</posx>
                    <posy>469</posy>
                    <width>150</width>
                    <height>70</height>
                    <texture>dialog_bottom.png</texture>
                </control>
                <control type="image" id="6">
                    <description>CD Cover Image</description>
                    <posx>65</posx>
                    <posy>377</posy>
                    <width>140</width>
                    <height>140</height>
                    <info>MusicPlayer.Cover</info>
                    <aspectratio>keep</aspectratio>
                </control>
            </control>

            <control type="group">
                <posx>350</posx>
                <posy>400</posy>
                <!--control type="image">
                    <description>Dialog Footer</description>
                    <posx>212</posx>
                    <posy>52</posy>
                    <width>120</width>
                    <height>70</height>
                    <texture>dialog_bottom.png</texture>
                </control>
                <control type="image">
                    <description>media info background image</description>
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>320</width>
                    <height>105</height>
                    <colordiffuse>BBFFFFFF</colordiffuse>
                    <texture border="10">dialog_middle.png</texture>
                </control-->
                <control type="fadelabel">
                    <description>Artist label</description>
                    <posx>240</posx>
                    <posy>78</posy>
                    <width>380</width>
                    <label>$INFO[MusicPlayer.Artist]</label>
                    <align>left</align>
                    <textcolor>FF9FCA4F</textcolor>
                    <font>font13</font>
                </control>
                <control type="label" id="8">
                    <description>Scraper</description>
                    <posx>655</posx>
                    <posy>78</posy>
                    <width>200</width>
                    <label>-</label>
                    <align>right</align>
                    <font>font12</font>
                    <textcolor>FF9FCA4F</textcolor>
                </control>
                <control type="fadelabel">
                    <description>Title label</description>
                    <posx>240</posx>
                    <posy>99</posy>
                    <width>380</width>
                    <label>$INFO[MusicPlayer.Title]</label>
                    <textcolor>FFE7FF00</textcolor>
                    <align>left</align>
                    <font>font14</font>
                </control>
                <control type="fadelabel">
                    <description>Album label</description>
                    <posx>240</posx>
                    <posy>125</posy>
                    <width>380</width>
                    <label>$INFO[MusicPlayer.Album]</label>
                    <textcolor>FF9FCA4F</textcolor>
                    <align>left</align>
                    <font>font12</font>
                </control>
                <control type="fadelabel">
                    <description>time label</description>
                    <posx>240</posx>
                    <posy>148</posy>
                    <width>380</width>
                    <label>$INFO[musicplayer.Time]$INFO[musicplayer.Duration, / ]</label>
                    <align>left</align>
                    <textcolor>FF9FCA4F</textcolor>
                    <font>font12</font>
                </control>
                <control type="label">
                    <description>Playlist Pos No</description>
                    <posx>655</posx>
                    <posy>148</posy>
                    <width>200</width>
                    <label>$INFO[musicplayer.Playlistposition,$LOCALIZE[435] ]$INFO[musicplayer.Playlistlength, / ]</label>
                    <visible>!MusicPartyMode.Enabled</visible>
                    <align>right</align>
                    <font>font12</font>
                    <textcolor>FF9FCA4F</textcolor>
                </control>
                <control type="label">
                    <description>PartyMode Enabled</description>
                    <posx>655</posx>
                    <posy>148</posy>
                    <width>200</width>
                    <label>589</label>
                    <visible>MusicPartyMode.Enabled</visible>
                    <align>right</align>
                    <font>font12</font>
                    <textcolor>FF9FCA4F</textcolor>
                </control>
            </control>
        </control>
    </controls>
</window>