<window>

 <id>1156</id>
 <allowoverlay>no</allowoverlay>
 <defaultcontrol always="false">4</defaultcontrol>
 <resolution>PAL</resolution>

  <coordinates>
  <system>1</system>
  <posx>155</posx>
  <posy>95</posy>
 </coordinates>
 
 <controls>

  <control type="image" id="1">
   <description>background image</description>
   <posx>0</posx>
   <posy>0</posy>
   <width>470</width>
   <height>433</height>
   <texture>dialog-panel.png</texture>
  </control>

  <control type="fadelabel" id="3">
   <description>artist and title</description>
   <posx>50</posx>
   <posy>13</posy>
   <width>635</width>
   <height>25</height>
   <label>$INFO[MusicPlayer.Artist] - $INFO[MusicPlayer.Title]</label>
   <font>special13</font>
   <textcolor>DDced8da</textcolor>
  </control>

  <control type="textbox" id="4">
   <description>textarea</description>
   <posx>45</posx>
   <posy>55</posy>
   <width>400</width>
   <height>360</height>
   <spinposx>410</spinposx>
   <spinposy>370</spinposy>
   <spinposx>410</spinposx>
   <spinposy>305</spinposy>
   <spincolor>ffffffff</spincolor>
   <spinfont>font10</spinfont>
   <textureup>scroll-up.png</textureup>
   <texturedown>scroll-down.png</texturedown>
   <textureupfocus>scroll-up-focus.png</textureupfocus>
   <texturedownfocus>scroll-down-focus.png</texturedownfocus>
   <image>icon-folder.png</image>
   <font>font13</font>
   <textcolor>ffffffff</textcolor>
   <visible>True</visible>
  </control>

  <control type="list" id="5">
   <description>link list</description>
   <posx>40</posx>
   <posy>55</posy>
   <width>408</width>
   <height>335</height>
   <spinposx>418</spinposx>
   <spinposy>345</spinposy>
   <texturefocus>list-focus.png</texturefocus>
   <texturenofocus>list-nofocus.png</texturenofocus>
   <textcolor>ffffffff</textcolor>
   <visible>False</visible>
  </control>

  <control type="image" id="6">
    <description>Cover</description>
    <posx>20</posx>
    <posy>13</posy>
    <width>25</width>
    <height>25</height>
    <texture>-</texture>
    <info>MusicPlayer.Cover</info>
    <aspectratio>keep</aspectratio>
  </control>

 </controls>

</window>