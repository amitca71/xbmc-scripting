		</control>

		<control type="image">
			<description>Dialog Header</description>
			<posx>-70</posx>
			<posy>-42</posy>
			<width>315</width>
			<height>180</height>
			<texture>dialog_top.png</texture>
			<animation effect="slide" time="150" start="120,120" end="0,0" delay="0" condition="!Skin.HasSetting(animated)">WindowOpen</animation>
		</control>
		
        <control type="image">
			<description>Dialog Text Area</description>
			<posx>-30</posx>
			<posy>-2</posy>
			<width>490</width>
			<height>360</height>
			<texture border="5">dialog_middle.png</texture>
