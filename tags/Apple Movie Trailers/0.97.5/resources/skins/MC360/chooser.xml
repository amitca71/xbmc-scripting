<window>

	
	<resolution>PAL</resolution>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>

	<controls>

		<!-- ** Optional ** Do not change <description> -->
		<control type="image" id="600">
			<description>Chooser Image Top</description>
			<posx>300</posx>
			<posy>0</posy>
			<width>512</width>
			<height>576</height>
			<texture>background-grey.png</texture>
		</control>
		<!-- ** Optional ** Do not change <description> or <type> -->
		<control type="image" id="601">
			<description>Chooser Image Middle</description>
			<posx>398</posx>
			<posy>0</posy>
			<width>400</width>
			<height>576</height>
			<texture>guide-blade-overlay-tile.png</texture>
		</control>
		<!-- ** Optional ** Do not change <description> -->
		<control type="image" id="602">
			<description>Chooser Image Bottom</description>
			<posx>270</posx>
			<posy>0</posy>
			<width>128</width>
			<height>576</height>
			<texture>guide-blade-overlay-edge-right.png</texture>
		</control>
		<!-- skin selection dialog -->
		<!-- ** Required ** Do not change <description> -->
		<control type="label" id="500">
			<description>Chooser Label</description>
			<posx>365</posx>
			<posy>40</posy>
			<width>270</width>
			<align>left</align>
			<font>font18</font>
			<textcolor>FFFFFFFF</textcolor>
			<label>-</label>
		</control>
		<!-- ** Required ** Do not change <description> or <type> -->
		<control type="image" id="501">
			<description>Chooser Thumb</description>
			<posx>380</posx>
			<posy>300</posy>
			<width>300</width>
			<height>150</height>
			<texture>-</texture>
			<aspectratio>keep</aspectratio>
		</control>
		<!-- ** Required ** Do not change <description> -->
		<control type="label" id="502">
			<description>Chooser Warning Label</description>
			<posx>380</posx>
			<posy>260</posy>
			<width>300</width>
			<height>20</height>
			<align>left</align>
			<font>font16</font>
			<label>231</label>
		</control>
		<!-- ** Required ** Do not change <description> or <type> -->
		<control type="list" id="503">
			<description>Chooser List1</description>
			<posx>365</posx>
			<posy>100</posy>
			<width>325</width>
			<height>150</height>
			<spinwidth>18</spinwidth>
			<spinheight>16</spinheight>
			<spinposx>280</spinposx>
			<spinposy>345</spinposy>
			<spincolor>FF000000</spincolor>
			<spinfont>font12</spinfont>
			<textureup>sortup-nofocus.png</textureup>
			<texturedown>sortdown-nofocus.png</texturedown>
			<textureupfocus>sortup-focus.png</textureupfocus>
			<texturedownfocus>sortdown-focus.png</texturedownfocus>
			<texturefocus>iconlist-focus.png</texturefocus>
			<texturenofocus>iconlist-nofocus.png</texturenofocus>
			<image>-</image>
			<textureheight>26</textureheight>
			<itemwidth>0</itemwidth>
			<font>font13</font>
			<font2>font12</font2>
			<textcolor>ff000000</textcolor>
			<suffix>|</suffix>
			<textxoff>0</textxoff>
			<textxoff2>0</textxoff2>
			<aligny>center</aligny>
		</control>
		<control type="list" id="504">
			<description>Chooser List2</description>
			<posx>365</posx>
			<posy>100</posy>
			<width>325</width>
			<height>360</height>
			<spinwidth>18</spinwidth>
			<spinheight>16</spinheight>
			<spinposx>280</spinposx>
			<spinposy>345</spinposy>
			<spincolor>FF000000</spincolor>
			<spinfont>font12</spinfont>
			<textureup>sortup-nofocus.png</textureup>
			<texturedown>sortdown-nofocus.png</texturedown>
			<textureupfocus>sortup-focus.png</textureupfocus>
			<texturedownfocus>sortdown-focus.png</texturedownfocus>
			<texturefocus>iconlist-focus.png</texturefocus>
			<texturenofocus>iconlist-nofocus.png</texturenofocus>
			<image>-</image>
			<textureheight>26</textureheight>
			<itemwidth>0</itemwidth>
			<font>font13</font>
			<font2>font12</font2>
			<textcolor>ff000000</textcolor>
			<suffix>|</suffix>
			<textxoff>0</textxoff>
			<textxoff2>0</textxoff2>
			<aligny>center</aligny>
		</control>

	</controls>
</window>