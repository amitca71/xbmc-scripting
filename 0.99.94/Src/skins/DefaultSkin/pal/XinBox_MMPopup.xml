<window>
	<defaultcontrol>50</defaultcontrol>
	<controls>	
		<control type="image">
			<posx>275</posx>
			<posy>100</posy>
			<width>390</width>
			<height>390</height>
			<texture>XBmmemopen.png</texture>
			<animation effect="slide" start="300,0" time="800">Visible</animation>
			<animation effect="slide" end="300,0" time="800">Hidden</animation>
			<visible>Skin.HasSetting(mmemail)</visible>
		</control>
        	<control type="textbox" id="72">
            		<description>fsmsgbody</description>
            		<posx>315</posx>
            		<posy>145</posy>
            		<width>280</width>
            		<height>320</height>
            		<spinposx>150</spinposx>
            		<spinposy>285</spinposy>
            		<textureup>XBscroll-up.png</textureup>
            		<texturedown>XBscroll-down.png</texturedown>
            		<textureupfocus>XBscroll-up-focus.png</textureupfocus>
            		<texturedownfocus>XBscroll-down-focus.png</texturedownfocus>
            		<font>font13</font>
            		<textcolor>FFFFFFFF</textcolor>
            		<onup>72</onup> 
            		<ondown>72</ondown>
            		<onleft>72</onleft>
           		<onright>62</onright>
			<animation effect="slide" start="300,0" time="800">Visible</animation>
			<animation effect="slide" end="300,0" time="800">Hidden</animation>
			<visible>Skin.HasSetting(mmemail)</visible>
        	</control>
        	<control type="fadelabel" id="73">
			<description>email subject and from</description>
			<animation effect="slide" start="300,0" time="800">Visible</animation>
			<animation effect="slide" end="300,0" time="800">Hidden</animation>
			<visible>Skin.HasSetting(mmemail)</visible>
			<width>300</width>
			<posx>312</posx>
			<posy>123</posy>
			<align>right</align>
			<font>special13</font>
			<textcolor>FFB2D4F5</textcolor>
		</control> 
		<control type="image">
			<posx>580</posx>
			<posy>100</posy>
			<width>390</width>
			<height>390</height>
			<texture>XBmmemopenPanel.png</texture>
			<animation effect="slide" start="100,0" time="500">Visible</animation>
			<animation effect="slide" end="100,0" time="500">Hidden</animation>
			<visible>Skin.HasSetting(mmemailpanel)</visible>
		</control>
  		<control type="label" id="61">
        		<description>Email List Label</description>
        		<posx>630</posx>
        		<posy>125</posy>
        		<width>150</width>
        		<label>-</label>
        		<font>special13</font>
        		<textcolor>FFB2D4F5</textcolor>
			<animation effect="slide" start="100,0" time="500">Visible</animation>
			<animation effect="slide" end="100,0" time="500">Hidden</animation>
			<visible>Skin.HasSetting(mmemailpanel)</visible>
  		</control>
		<control type="button" id="62">
			<animation effect="slide" start="100,0" time="500">Visible</animation>
			<animation effect="slide" end="100,0" time="500">Hidden</animation>
			<visible>Skin.HasSetting(mmemailpanel)</visible>
			<description>Open Box Button</description>
			<posx>625</posx>
			<posy>429</posy>
      			<textoffsetx></textoffsetx>
      			<textoffsety></textoffsety>
			<label>-</label>
			<onright>62</onright>
			<onup>50</onup>
			<ondown>62</ondown>
			<onleft>72</onleft>
			<align>center</align>
			<aligny>center</aligny>
      			<texturefocus>XBbuttonF.png</texturefocus>
			<texturenofocus>XBbuttonNF.png</texturenofocus>
			<disabledcolor>60ffffff</disabledcolor>
			<width>94</width>
			<height>20</height>
		</control>
		<control type="list" id="50">
			<animation effect="slide" start="100,0" time="500">Visible</animation>
			<animation effect="slide" end="100,0" time="500">Hidden</animation>
			<visible>Skin.HasSetting(mmemailpanel)</visible>
			<posx>620</posx>
			<posy>150</posy>
			<width>150</width>
			<height>275</height>
			<onleft>72</onleft>
			<onright>62</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<viewtype label="535">list</viewtype>
			<itemlayout height="27">
				<control type="image">
					<width>150</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>150</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(50)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
	</controls>
</window>