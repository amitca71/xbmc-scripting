<window>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
	<coordinates>
		<system>1</system>
		<posx>155</posx>
		<posy>150</posy>
	</coordinates>
	<controls>
		<control type="image" id="0">
			<posx>-155</posx>
			<posy>-150</posy>
			<width>720</width>
			<height>576</height>
			<texture>amt-black.png</texture>
			<!--animation effect="fade" time="2000">WindowOpen</animation-->
		</control>
		<control type="image" id="1">
			<description>background image</description>
			<posx>-15</posx>
			<posy>-15</posy>
			<width>440</width>
			<height>335</height>
			<texture>amt-dialog-panel.png</texture>
		</control>
		<!-- ** Required ** Do not change <description> -->
		<control type="label" id="500">
			<description>Heading Label</description>
			<posx>10</posx>
			<posy>-2</posy>
			<width>420</width>
			<align>left</align>
			<label>214</label>
			<font>special13</font>
			<textcolor>DDced8da</textcolor>
		</control>
 		<!-- skin selection controls -->
		<!-- ** Required ** Do not change <description> or <type> -->
        <control type="multiimage">
            <posx>200</posx>
            <posy>40</posy>
            <width>241</width>
            <height>135</height>
            <info>Skin.String(AMT-chooser-thumbfolder)</info>
            <timeperimage>2000</timeperimage>
            <fadetime>500</fadetime>
            <visible>!Control.IsVisible(504)</visible>
            <animation effect="fade" time="200">VisibleChange</animation>
			<aspectratio>keep</aspectratio>
        </control>
		<!-- ** Required ** Do not change <description> -->
		<control type="label" id="502">
			<description>Chooser Warning Label</description>
			<posx>200</posx>
			<posy>180</posy>
			<width>241</width>
			<height>20</height>
			<align>center</align>
			<font>font10</font>
			<textcolor>DDced8da</textcolor>
			<label>-</label>
		</control>
		<!-- ** Required ** Do not change <id> or <type> -->
		<control type="list" id="503">
			<posx>10</posx>
			<posy>40</posy>
			<width>210</width>
			<height>224</height>
			<onleft>513</onleft>
			<onright>513</onright>
			<onup>503</onup>
			<ondown>503</ondown>
			<viewtype label="535">list</viewtype>
			<pagecontrol>513</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="32">
				<control type="image">
					<width>210</width>
					<height>30</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture border="5">amt-input-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>0</posy>
					<width>200</width>
					<height>30</height>
					<font>font13</font>
					<aligny>center</aligny>
					<textcolor>DDFFFFFF</textcolor>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="32">
				<control type="image">
					<width>210</width>
					<height>30</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(503)</visible>
					<texture border="5">amt-input-focus.png</texture>
				</control>
				<control type="image">
					<width>210</width>
					<height>30</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>!Control.HasFocus(503)</visible>
					<texture border="5">amt-input-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>0</posy>
					<width>200</width>
					<height>30</height>
					<font>font13</font>
					<aligny>center</aligny>
					<textcolor>DDFFFFFF</textcolor>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="spincontrol" id="513">
			<description>Next page button</description>
			<posx>370</posx>
			<posy>270</posy>
			<subtype>page</subtype>
			<font>font10</font>
			<onleft>503</onleft>
			<onright>503</onright>
			<ondown>503</ondown>
			<onup>503</onup>
			<textcolor>ffffffff</textcolor>
			<textureup>amt-scroll-up.png</textureup>
			<texturedown>amt-scroll-down.png</texturedown>
			<textureupfocus>amt-scroll-up-focus.png</textureupfocus>
			<texturedownfocus>amt-scroll-down-focus.png</texturedownfocus>
			<showonepage>false</showonepage>
			<visible>Control.IsVisible(503)</visible>
		</control>
		<!-- ** Required ** Do not change <id> or <type> -->
