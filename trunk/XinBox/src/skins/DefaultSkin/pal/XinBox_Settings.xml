<window>
	<defaultcontrol>50</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<colorkey>0</colorkey>
			<textcolor>ffffffff</textcolor>
			<colordiffuse>ffffffff</colordiffuse>
			<texture>XBbackground.png</texture>
		</control>
		<control type="label">
			<description>description label</description>
			<posx>207</posx>
			<posy>43</posy>
			<align>left</align>
			<label>XinBox Settings WIP</label>
			<font>special13</font>
		</control>
		<control type="list" id="60">
			<posx>100</posx>
			<posy>125</posy>
			<width>455</width>
			<height>400</height>
			<viewtype label="535">list</viewtype>
			<itemlayout height="27">
				<control type="label">
					<posx>30</posx>
					<posy>2</posy>
					<width>380</width>
					<height>30</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
		</control>
		<control type="list" id="50">
			<posx>211</posx>
			<posy>125</posy>
			<width>455</width>
			<height>400</height>
			<onleft>9000</onleft>
			<onright>69</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<viewtype label="535">list</viewtype>
			<pagecontrol>69</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>2</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(50)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>2</posy>
					<width>380</width>
					<height>30</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
	</controls>
</window>