<window>
	<defaultcontrol>72</defaultcontrol>
	<controls>
		<control type="image" id="78">
			<description>attachpanel</description>
			<posx>250</posx>
			<posy>100</posy>
			<width>500</width>
			<visible>Skin.HasSetting(attachlistnotempty)</visible>
			<height>400</height>
			<texture>XBattach.png</texture>
		</control>
		<control type="image" id="71">
			<description>fsoverlay</description>
			<posx>100</posx>
			<posy>100</posy>
			<width>500</width>
			<height>400</height>
			<texture>XBfs.png</texture>
		</control>
        	<control type="label" id="79">
			<description>attachmentlabel</description>
			<width>405</width>
			<posx>600</posx>
			<posy>130</posy>
			<align>left</align>
			<font>font18</font>
			<visible>Skin.HasSetting(attachlistnotempty)</visible>
			<textcolor>FFB2D4F5</textcolor>
		</control> 
        	<control type="textbox" id="72">
            		<description>fsmsgbody</description>
            		<posx>150</posx>
            		<posy>150</posy>
            		<width>390</width>
            		<height>300</height>
            		<spinposx>240</spinposx>
            		<spinposy>290</spinposy>
            		<textureup>XBscroll-up.png</textureup>
            		<texturedown>XBscroll-down.png</texturedown>
            		<textureupfocus>XBscroll-up-focus.png</textureupfocus>
            		<texturedownfocus>XBscroll-down-focus.png</texturedownfocus>
            		<font>font13</font>
            		<textcolor>FFFFFFFF</textcolor>
            		<onup>72</onup> 
            		<ondown>72</ondown>
            		<onleft>72</onleft>
           		<onright>75</onright>
        	</control>	
		<control type="list" id="77">
			<posx>560</posx>
			<posy>150</posy>
			<width>140</width>
			<height>120</height>
			<onleft>76</onleft>
			<onright>77</onright>
			<onup>77</onup>
			<ondown>77</ondown>
			<viewtype label="535">list</viewtype>
			<visible>Skin.HasSetting(attachlistnotempty)</visible>
			<itemlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>5</posx>
					<posy>3</posy>
					<width>20</width>
					<height>20</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>430</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>150</width>
					<height>25</height>
					<posx>-5</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(77)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>20</width>
					<height>20</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>0</posx>
					<posy>0</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>430</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<textcolor>60ffffff</textcolor>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>
        	<control type="fadelabel" id="73">
			<description>emailinfo</description>
			<width>405</width>
			<posx>150</posx>
			<posy>130</posy>
			<align>left</align>
			<font>font18</font>
			<textcolor>FFB2D4F5</textcolor>
		</control> 
		<control type="fadelabel" id="74">
			<description>emailrec</description>
			<posx>150</posx>
			<posy>440</posy>
			<width>210</width>
			<align>left</align>
			<font>font18</font>
			<textcolor>FFB2D4F5</textcolor>
		</control>
		<control type="button" id="75">
			<description>Attach button</description>
			<posx>430</posx>
			<posy>440</posy>
			<label>A</label>
			<width>17</width>
			<height>17</height>
			<onleft>72</onleft>
			<onright>76</onright>
			<onup>75</onup>
			<textoffsetx>5</textoffsetx>
			<ondown>75</ondown>
			<disabledcolor>60ffffff</disabledcolor>
		</control>
		<control type="button" id="76">
			<description>Del button</description>
			<posx>447</posx>
			<textoffsetx>5</textoffsetx>
			<posy>440</posy>
			<label>D</label>
			<width>17</width>
			<height>17</height>
			<onleft>75</onleft>
			<onright>77</onright>
			<onup>76</onup>
			<ondown>76</ondown>
		</control>
		<control type="image" id="80">
			<description>attachimage</description>
			<posx>565</posx>
			<posy>280</posy>
			<width>130</width>
			<height>170</height>
			<texture>-</texture>
			<visible>Skin.HasSetting(attachlistnotempty)</visible>
			<aspectratio>keep</aspectratio>
		</control>

    </controls>

</window>
