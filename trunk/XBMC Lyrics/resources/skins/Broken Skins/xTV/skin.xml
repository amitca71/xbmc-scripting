<window id="2006">
    <allowoverlay>no</allowoverlay>
    <defaultcontrol always="false">4</defaultcontrol>
    <resolution>PAL</resolution>
    <useincludes>true</useincludes>
    <coordinates>
        <system>1</system>
        <posx>0</posx>
        <posy>0</posy>
    </coordinates>

    <controls>
        <control type="image" id="1">
            <description>background image</description>
            <posx>50</posx>
            <posy>50</posy>
            <width>620</width>
            <height>476</height>
            <texture>button-menu-background.png</texture>
        </control>

        <control type="image" id="1">
            <description>divider image</description>
            <posx>190</posx>
            <posy>182</posy>
            <width>460</width>
            <height>4</height>
            <texture>separator.png</texture>
        </control>
        
		<!-- ** Required ** Do not change <id> or <type> -->
        <control type="textbox" id="3">
            <description>textarea</description>
            <posx>80</posx>
            <posy>195</posy>
            <width>570</width>
            <height>320</height>
            <onup>3</onup>
            <ondown>3</ondown>
            <onleft>3</onleft>
            <onright>3</onright>
            <spinposx>350</spinposx>
            <spinposy>345</spinposy>
            <spincolor>ffffffff</spincolor>
            <textureup>scroll-up.png</textureup>
            <texturedown>scroll-down.png</texturedown>
            <textureupfocus>scroll-up-focus.gif</textureupfocus>
            <texturedownfocus>scroll-down-focus.gif</texturedownfocus>
            <font>font13</font>
            <include>button-text</include>
            <visible>false</visible>
        </control>

		<!-- ** Required ** Do not change <id> or <type> -->
        <control type="list" id="4">
            <description>lyrics list</description>
            <posx>80</posx>
            <posy>195</posy>
            <width>590</width>
            <height>320</height>
            <onleft>4</onleft>
            <onright>4</onright>
            <onup>4</onup>
            <ondown>4</ondown>
            <spinposx>410</spinposx>
            <spinposy>345</spinposy>
            <spincolor>ddffffff</spincolor>
            <textureup>scroll-up.png</textureup>
            <texturedown>scroll-down.png</texturedown>
            <textureupfocus>scroll-up-focus.png</textureupfocus>
            <texturedownfocus>scroll-down-focus.png</texturedownfocus>
            <texturefocus>default-button.png</texturefocus>
            <texturenofocus>-</texturenofocus>
            <image>-</image>
	    <itemwidth>0</itemwidth>
	    <itemheight>0</itemheight>
            <textureheight>22</textureheight>
            <spacebetweenitems>1</spacebetweenitems>  
            <font>font13</font>
            <include>listselectedtext</include>
            <include>button-text</include>
            <textcolor2>ddffffff</textcolor2>
            <suffix> |</suffix>
            <textxoff>3</textxoff>
            <textxoff2>0</textxoff2>
            <aligny>center</aligny>
            <visible>false</visible>
			<textxoff>0</textxoff>
        </control>

		<!-- ** Required ** Do not change <id> or <type> -->
        <control type="list" id="5">
            <description>link list</description>
            <posx>80</posx>
            <posy>195</posy>
            <width>590</width>
            <height>320</height>
            <onleft>5</onleft>
            <onright>5</onright>
            <onup>5</onup>
            <ondown>5</ondown>
            <spinposx>410</spinposx>
            <spinposy>345</spinposy>
            <spincolor>ddffffff</spincolor>
            <textureup>scroll-up.png</textureup>
            <texturedown>scroll-down.png</texturedown>
            <textureupfocus>scroll-up-focus.png</textureupfocus>
            <texturedownfocus>scroll-down-focus.png</texturedownfocus>
            <texturefocus>default-button.png</texturefocus>
            <texturenofocus>-</texturenofocus>
            <image>-</image>
	    <itemwidth>0</itemwidth>
	    <itemheight>0</itemheight>
            <textureheight>29</textureheight>
            <spacebetweenitems>2</spacebetweenitems>  
	    <textxoff>0</textxoff>
            <font>font13</font>
            <include>listselectedtext</include>
            <include>button-text</include>
            <textcolor2>ddffffff</textcolor2>
            <colordiffuse>ffffffff</colordiffuse>
            <suffix> |</suffix>
            <textxoff>3</textxoff>
            <textxoff2>0</textxoff2>
            <aligny>center</aligny>
            <visible>false</visible>
        </control>

		<!-- ** Optional ** Do not change <id> or <type> -->
        <control type="label" id="7">
            <description>list label</description>
            <posx>647</posx>
            <posy>481</posy>
            <width>100</width>
            <label>1/4</label>
            <align>right</align>
            <font>font13</font>
            <textcolor>00000000</textcolor>
            <visible>false</visible>
        </control>

        <control type="group">
            <visible>Player.ShowInfo</visible>
            <animation effect="fade" time="1000">VisibleChange</animation>
            <control type="group">
                <posx>40</posx>
                <posy>350</posy>
                <control type="image">
                    <description>Dialog Text Area</description>
                    <posx>60</posx>
                    <posy>340</posy>
                    <width>150</width>
                    <height>175</height>
                    <texture>-</texture>
                </control>
                
                <!-- ** Required ** Do not change <id> or <type> -->
                <control type="image" id="6">
                    <description>CD Cover Image</description>
                    <posx>65</posx>
                    <posy>65</posy>
                    <width>120</width>
                    <height>120</height>
                    <info>MusicPlayer.Cover</info>
                    <aspectratio>keep</aspectratio>
                </control>
            </control>

            <control type="group">
                <posx>350</posx>
                <posy>400</posy>
                <control type="fadelabel">
                    <description>Artist label</description>
                    <posx>190</posx>
                    <posy>65</posy>
                    <width>380</width>
                    <label>$INFO[MusicPlayer.Artist]</label>
                    <align>left</align>
                    <include>info-text</include>
                    <font>font13</font>
                </control>
                <control type="fadelabel">
                    <description>Title label</description>
                    <posx>190</posx>
                    <posy>86</posy>
                    <width>380</width>
                    <label>$INFO[MusicPlayer.Title]</label>
                    <include>button-text</include>
                    <align>left</align>
                    <font>font14</font>
                </control>
                <control type="fadelabel">
                    <description>Album label</description>
                    <posx>190</posx>
                    <posy>107</posy>
                    <width>380</width>
                    <label>$INFO[MusicPlayer.Album]</label>
                    <include>info-text</include>
                    <align>left</align>
                    <font>font12</font>
                </control>
                <control type="fadelabel">
                    <description>time label</description>
                    <posx>190</posx>
                    <posy>128</posy>
                    <width>380</width>
                    <label>$INFO[musicplayer.Time]$INFO[musicplayer.Duration, / ]</label>
                    <align>left</align>
                    <include>info-text</include>
                    <font>font12</font>
                </control>
                <control type="label">
                    <description>Playlist Pos No</description>
                    <posx>190</posx>
                    <posy>149</posy>
                    <width>200</width>
                    <label>$INFO[musicplayer.Playlistposition,$LOCALIZE[435] ]$INFO[musicplayer.Playlistlength, / ]</label>
                    <visible>!MusicPartyMode.Enabled</visible>
                    <align>left</align>
                    <font>font12</font>
                    <include>info-text</include>
                </control>
                <control type="label">
                    <description>PartyMode Enabled</description>
                    <posx>300</posx>
                    <posy>149</posy>
                    <width>200</width>
                    <label>Party Mode</label>
                    <visible>MusicPartyMode.Enabled</visible>
                    <align>left</align>
                    <font>font12</font>
                    <include>info-text</include>
                </control>
                <control type="label" id="8">
		    <description>Scraper</description>
		    <posx>655</posx>
		    <posy>149</posy>
		    <width>200</width>
		    <label>-</label>
		    <align>right</align>
		    <font>font12</font>
		    <include>info-text</include>
                </control>
            </control>
        </control>
    </controls>
</window>