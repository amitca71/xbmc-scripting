<window>

	<id>1156</id>
	<defaultcontrol>4</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<resolution>PAL</resolution>

	<controls>

	        <control type="image" id="1">
			<description>Black translucent backdrop</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<texture>guide-black.png</texture>
			<colordiffuse>5DFFFFFF</colordiffuse>
			<animation effect="fade" delay="450" time="100">WindowClose</animation>
		</control>

	        <control type="image" id="1">
			<description>Guide Blade Background</description>
			<posx>30</posx>
			<posy>0</posy>
			<width>690</width>
			<height>576</height>
			<visible>!Skin.String(GuidePanel)</visible>
			<colordiffuse>BEFFFFFF</colordiffuse>
			<texture>background-grey.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>

	        <control type="image" id="1">
			<description>Custom Guide Blade Background</description>
			<posx>30</posx>
			<posy>0</posy>
			<width>690</width>
			<height>576</height>
			<texture>-</texture>
			<colordiffuse>BEFFFFFF</colordiffuse>
			<info>Skin.String(GuidePanel)</info>
			<texture>background-grey.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>

	        <control type="image" id="1">
			<description>Guide Blade Overlay</description>
			<posx>128</posx>
			<posy>0</posy>
			<width>592</width>
			<height>576</height>
			<colordiffuse>FFFFFFFF</colordiffuse>
			<texture>guide-blade-overlay-tile.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>
	        <control type="image" id="1">
			<description>Guide Edge Overlay</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>128</width>
			<height>576</height>
			<colordiffuse>FFFFFFFF</colordiffuse>
			<texture>guide-blade-overlay-edge-right.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>

	        <control type="image" id="1">
			<description>Back Button</description>
			<posx>630</posx>
			<posy>475</posy>
			<width>21</width>
			<height>21</height>
			<texture>button-Back.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control> 

	        <control type="image" id="1">
			<description>A Button</description>
			<posx>645</posx>
			<posy>498</posy>
			<width>21</width>
			<height>21</height>
			<texture>button-A.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control> 

	        <control type="image" id="1">
			<description>Y Button</description>
			<posx>110</posx>
			<posy>475</posy>
			<width>21</width>
			<height>21</height>
			<texture>button-Y.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control> 

		<control type="label">
			<description>Setting Label</description>
			<posx>140</posx>
			<posy>478</posy>
			<label>Settings</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FFFFFFFF</textcolor>
		</control>

	        <control type="image" id="1">
			<description>X Button</description>
			<posx>95</posx>
			<posy>498</posy>
			<width>21</width>
			<height>21</height>
			<texture>button-X-turnedoff.png</texture>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control> 

	        <control type="label" id="1">
			<description>Back Label</description>
			<posx>589</posx>
			<posy>479</posy>
			<label>Back</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FFFFFFFF</textcolor>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>

	        <control type="label" id="1">
			<description>Select Label</description>
			<posx>594</posx>
			<posy>501</posy>
			<label>Select</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FFFFFFFF</textcolor>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>


		<control type="label" id="1">
			<description>description label</description>
			<posx>80</posx>
			<posy>200</posy>
			<align>left</align>
			<label>Lyrics</label>
			<textcolor>FF000000</textcolor>
			<include>shadow-grey</include>
			<font>font14</font>
			<angle>270</angle>
			<animation effect="slide" time="250" start="212,0">WindowOpen</animation>
			<animation effect="slide" time="250" delay="250" end="720,0">WindowClose</animation>
		</control>

	        <control type="fadelabel" id="3">
			<description>Album Title value</description>
			<posx>115</posx>
			<posy>35</posy>
			<width>580</width>
			<align>left</align>
			<label>$INFO[MusicPlayer.Artist] - $INFO[MusicPlayer.Title]</label>
			<textcolor>FFFFFFFF</textcolor>
			<shadowcolor>FF000000</shadowcolor>
			<font>font20</font>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="150">WindowClose</animation>
		</control>

	        <control type="textbox" id="4">
			<description>textarea</description>
			<posx>315</posx>
			<posy>90</posy>
			<width>380</width>
			<height>380</height>
			<spinwidth>18</spinwidth>
			<spinheight>16</spinheight>
			<spinposx>362</spinposx>
			<spinposy>160</spinposy>
			<spincolor>FF000000</spincolor>
			<spinfont>font12</spinfont>
			<textureup>sortup-nofocus.png</textureup>
			<texturedown>sortdown-nofocus.png</texturedown>
			<textureupfocus>sortup-focus.png</textureupfocus>
			<texturedownfocus>sortdown-focus.png</texturedownfocus>
			<textcolor>FF000000</textcolor>
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<font>font14</font>
			<visible>True</visible>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="150">WindowClose</animation>
		</control>

	        <control type="list" id="5">
			<description>listcontrol</description>
			<posx>315</posx>
			<posy>90</posy>
			<width>370</width>
			<height>350</height>
			<spinwidth>18</spinwidth>
			<spinheight>16</spinheight>
			<spinposx>362</spinposx>
			<spinposy>160</spinposy>
			<spincolor>FF000000</spincolor>
			<spinfont>font12</spinfont>
			<textureup>sortup-nofocus.png</textureup>
			<texturedown>sortdown-nofocus.png</texturedown>
			<textureupfocus>sortup-focus.png</textureupfocus>
			<texturedownfocus>sortdown-focus.png</texturedownfocus>
			<texturefocus>iconlist-focus.png</texturefocus>
			<texturenofocus>iconlist-nofocus.png</texturenofocus>
			<textcolor>FF000000</textcolor>
			<textureheight>26</textureheight>
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<shadowcolor>a0dadada</shadowcolor>
			<font>font13</font>
			<textxoff>-10</textxoff>
			<textxoff2>0</textxoff2>
			<aligny>center</aligny>
			<visible>False</visible>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="150">WindowClose</animation>
		</control>

	        <control type="image" id="6">
			<description>Cover</description>
			<posx>130</posx>
			<posy>92</posy>
			<width>161</width>
			<height>176</height>
			<texture>-</texture>
			<info>MusicPlayer.Cover</info>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="150">WindowClose</animation>
		</control>

	        <control type="image" id="1">
			<description>Jewel CD Case</description>
			<posx>98</posx>
			<posy>82</posy>
			<width>205</width>
			<height>203</height>
			<texture>music-overlay-cdcase.png</texture>
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="150">WindowClose</animation>
		</control>

	        <control type="fadelabel" id="1">
			<description>Music - Time</description>
			<posx>105</posx>
			<posy>285</posy>
			<width>170</width>
			<label>Time : $INFO[MusicPlayer.Time] - $INFO[MusicPlayer.Duration]</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FF000000</textcolor>
			<shadowcolor>a0dadada</shadowcolor>
			<animation effect="fade" time="100">WindowOpen</animation>
		</control>

	        <control type="label" id="1">
			<description>Music - Album</description>
			<posx>105</posx>
			<posy>307</posy>
			<width>170</width>
			<label>Album : $INFO[MusicPlayer.Album]</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FF000000</textcolor>
			<shadowcolor>a0dadada</shadowcolor>
			<animation effect="fade" time="100">WindowOpen</animation>
		</control>

	        <control type="fadelabel" id="1">
			<description>Music - Codec</description>
			<posx>105</posx>
			<posy>329</posy>
			<width>170</width>
			<label>Codec : $INFO[MusicPlayer.Codec]</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FF000000</textcolor>
			<shadowcolor>a0dadada</shadowcolor>
			<animation effect="fade" time="100">WindowOpen</animation>
		</control>

	        <control type="fadelabel" id="1">
			<description>Music - Bitrate</description>
			<posx>105</posx>
			<posy>351</posy>
			<width>170</width>
			<label>Bitrate : $INFO[MusicPlayer.Bitrate] kbps</label>
			<align>left</align>
			<font>font12</font>
			<textcolor>FF000000</textcolor>
			<shadowcolor>a0dadada</shadowcolor>
			<animation effect="fade" time="100">WindowOpen</animation>
		</control>

	</controls>

</window>
