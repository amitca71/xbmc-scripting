<window>
	<defaultcontrol>51</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="image">
			<description>Background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<texture>XBbackground-bottom.png</texture>
		</control>
		<control type="image">
			<description>Menu Open</description>
			<posx>153</posx>
			<posy>60</posy>
			<width>400</width>
			<height>63</height>
			<texture>XBmenu-top.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
		<control type="image">
			<description>XinBox Logo image</description>
			<posx>300</posx>
			<posy>45</posy>
			<width>120</width>
			<height>120</height>
			<texture>XBXinBoXLogo.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
		<control type="image">
			<description>Menu Icon image</description>
			<posx>175</posx>
			<posy>60</posy>
			<width>65</width>
			<height>65</height>
			<texture>XBcreatenew.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
  		<control type="label" id="80">
        		<description>Menu Label</description>
        		<posx>408</posx>
        		<posy>95</posy>
        		<width>150</width>
        		<label>-</label>
        		<font>special13</font>
			<align>center</align>
        		<textcolor>FFB2D4F5</textcolor>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
  		</control>	
  		<control type="label" id="81">
        		<description>Version Label</description>
        		<posx>170</posx>
        		<posy>60</posy>
			<label>-</label>
        		<width>100</width>
        		<font>font13</font>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
  		</control>		
		<control type="image">
			<description>Panel</description>
			<posx>140</posx>
			<posy>120</posy>
			<width>430</width>
			<height>450</height>
			<texture>XBcreateaccountmenu.png</texture>
		</control>
		<control type="list" id="51">
			<posx>163</posx>
			<posy>140</posy>
			<width>290</width>
			<height>275</height>
			<onright>69</onright>
			<scrolltime>200</scrolltime>
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
			<itemlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>-3</posx>
					<posy>0</posy>
					<width>25</width>
					<height>25</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>2</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>285</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>290</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(51)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="image">
					<posx>-3</posx>
					<posy>0</posy>
					<width>25</width>
					<height>25</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>2</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>285</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>

    </controls>

</window>
