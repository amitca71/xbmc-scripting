<window>
	<defaultcontrol>61</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="image">
			<description>Menu Open</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>710</width>
			<height>90</height>
			<texture>XBmenu-top.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
		<control type="image">
			<description>Menu Icon image</description>
			<posx>300</posx>
			<posy>0</posy>
			<width>80</width>
			<height>80</height>
			<texture>XBlogin.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
		<control type="image">
			<description>XinBox Logo image</description>
			<posx>50</posx>
			<posy>-20</posy>
			<width>150</width>
			<height>150</height>
			<texture>XBXinBoXLogo.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
  		<control type="label" id="80">
        		<description>Menu Label</description>
        		<posx>440</posx>
        		<posy>0</posy>
        		<width>100</width>
        		<label>-</label>
        		<font>special13</font>
        		<textcolor>FFB2D4F5</textcolor>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
  		</control>
		<control type="image">
			<description>Inbox Background</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<texture>XBinbox.png</texture>
		</control>	
		<control type="group" id="9000">
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
			<posx>7</posx>
			<posy>88</posy>
			<control type="button" id="61">
				<description>Check Button</description>
				<posx>0</posx>
				<posy>0</posy>
      				<textoffsetx>2</textoffsetx>
      				<textoffsety></textoffsety>
				<label>-</label>
				<onright>50</onright>
				<onup>61</onup>
				<ondown>62</ondown>
				<onleft>61</onleft>
				<align>center</align>
				<aligny>center</aligny>
      				<texturefocus>XBlist-focus.png</texturefocus>
				<texturenofocus>XBlist-nofocus.png</texturenofocus>
				<disabledcolor>60ffffff</disabledcolor>
				<width>108</width>
				<height>37</height>
			</control>
			<control type="button" id="62">
				<description>Compose Button</description>
				<posx>0</posx>
				<posy>57</posy>
      				<textoffsetx>2</textoffsetx>
      				<textoffsety></textoffsety>
				<label>-</label>
				<onup>61</onup>
				<onleft>62</onleft>
				<onright>50</onright>
				<ondown>63</ondown>
				<align>center</align>
				<aligny>center</aligny>
      				<texturefocus>XBlist-focus.png</texturefocus>
				<texturenofocus>XBlist-nofocus.png</texturenofocus>
				<disabledcolor>60ffffff</disabledcolor>
				<width>108</width>
				<height>37</height>
			</control>
			<control type="button" id="63">
				<description>MiniMode Button</description>
				<posx>0</posx>
				<posy>379</posy>
      				<textoffsetx>2</textoffsetx>
      				<textoffsety></textoffsety>
				<label>-</label>
				<onup>62</onup>
				<onleft>63</onleft>
				<onright>50</onright>
				<ondown>64</ondown>
				<align>center</align>
				<aligny>center</aligny>
      				<texturefocus>XBlist-focus.png</texturefocus>
				<texturenofocus>XBlist-nofocus.png</texturenofocus>
				<disabledcolor>60ffffff</disabledcolor>
				<width>108</width>
				<height>37</height>
			</control>
			<control type="button" id="64">
				<description>Exit Button</description>
				<posx>0</posx>
				<posy>436</posy>
      				<textoffsetx>2</textoffsetx>
      				<textoffsety></textoffsety>
				<label>-</label>
				<onup>63</onup>
				<onleft>64</onleft>
				<onright>50</onright>
				<ondown>64</ondown>
				<align>center</align>
				<aligny>center</aligny>
      				<texturefocus>XBlist-focus.png</texturefocus>
				<texturenofocus>XBlist-nofocus.png</texturenofocus>
				<disabledcolor>60ffffff</disabledcolor>
				<width>108</width>
				<height>37</height>
			</control>
		</control>	
		<control type="list" id="50">
            	<posx>165</posx>
            	<posy>160</posy>
            	<width>400</width>
            	<height>400</height>
		<onleft>9000</onleft>
		<scrolltime>200</scrolltime>
		<animation effect="fade" time="1000">WindowOpen</animation>
		<animation effect="fade" time="1000">WindowClose</animation>
			<itemlayout height="70">
				<control type="image">
					<width>400</width>
					<height>70</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>70</width>
					<height>70</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>140</posx>
					<posy>100</posy>
					<width>32</width>
					<height>32</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="label">
					<posx>160</posx>
					<posy>20</posy>
					<width>330</width>
					<height>16</height>
					<align>center</align>
					<font>special13</font>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="70">
				<control type="image">
					<width>400</width>
					<height>70</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-focus.png</texture>
					<visible>Control.HasFocus(50)</visible>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>70</width>
					<height>70</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="image">
					<posx>140</posx>
					<posy>100</posy>
					<width>32</width>
					<height>32</height>
					<info>ListItem.Overlay</info>
				</control>
				<control type="label">
					<posx>160</posx>
					<posy>20</posy>
					<width>330</width>
					<height>16</height>
					<align>center</align>
					<font>special13</font>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>300</posx>
					<posy>30</posy>
					<width>350</width>
					<height>16</height>
					<font>font13</font>
					<align>center</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>

    </controls>

</window>
