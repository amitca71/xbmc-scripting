<window id="3000">
	<defaultcontrol>5</defaultcontrol>
	<controls>
		<include>LiveBackGroundWhitewash</include>
		<!-- Start Normal Window Controls -->
		<control type="group">
			<animation effect="fade" delay="100" time="250">WindowOpen</animation>
			<animation effect="fade" time="100">WindowClose</animation>
			<control type="label" id="1">
				<description>XBMC Live Place label</description>
				<posx>102</posx>
				<posy>42</posy>
				<align>left</align>
				<label>Changelog</label>
				<include>TextcolorWhite</include>
				<include>shadow-black</include>
				<font>font18</font>
			</control>
			<control type="label" id="6">
				<description>date label</description>
				<posx>640</posx>
				<posy>95</posy>
				<align>right</align>
				<label>-</label>
				<font>font12</font>
			</control>
			<control type="label" id="4">
				<description>header label</description>
				<posx>100</posx>
				<posy>95</posy>
				<width>550</width>
				<font>font12</font>
				<animation effect="fade" start="0" end="100" delay="200" time="100" condition="!Skin.HasSetting(animated)">WindowOpen</animation>
				<animation effect="fade" start="100" end="0" delay="0" time="100" condition="!Skin.HasSetting(animated)">WindowClose</animation>
			</control>
			<control type="textbox" id="5">
				<description>textarea</description>
				<posx>100</posx>
				<posy>140</posy>
				<width>550</width>
				<height>350</height>
				<font>font12</font>
				<spinposx>510</spinposx>
				<spinposy>335</spinposy>
				<animation effect="fade" start="0" end="100" delay="200" time="100" condition="!Skin.HasSetting(animated)">WindowOpen</animation>
				<animation effect="fade" start="100" end="0" delay="0" time="100" condition="!Skin.HasSetting(animated)">WindowClose</animation>
			</control>
			<include>SystemCustomKeymaps</include>
		</control>
		<include>LiveCommonsBlades</include>
	</controls>
</window>