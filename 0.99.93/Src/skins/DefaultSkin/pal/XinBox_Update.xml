<window>
	<defaultcontrol>50</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="image">
			<description>Background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<texture>XBbackground-bottom.png</texture>
		</control>
		<control type="image">
			<description>Menu Open</description>
			<posx>153</posx>
			<posy>60</posy>
			<width>400</width>
			<height>63</height>
			<texture>XBmenu-top.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
		<control type="image">
			<description>XinBox Logo image</description>
			<posx>160</posx>
			<posy>45</posy>
			<width>120</width>
			<height>120</height>
			<texture>XBXinBoXLogo.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
		<control type="image">
			<description>Menu Icon image</description>
			<posx>330</posx>
			<posy>60</posy>
			<width>65</width>
			<height>65</height>
			<texture>XBupdate.png</texture>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
		</control>
  		<control type="label" id="80">
        		<description>Menu Label</description>
        		<posx>400</posx>
        		<posy>95</posy>
        		<width>300</width>
        		<label>-</label>
        		<font>special13</font>
        		<textcolor>FFB2D4F5</textcolor>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
  		</control>	
  		<control type="label" id="81">
        		<description>Version Label</description>
        		<posx>435</posx>
        		<posy>65</posy>
			<label>-</label>
        		<width>300</width>
        		<font>font13</font>
			<animation effect="slide" start="0,100" time="800">WindowOpen</animation>
			<animation effect="slide" end="0,100" time="800">WindowClose</animation>
  		</control>		
		<control type="image">
			<description>Panel</description>
			<posx>140</posx>
			<posy>120</posy>
			<width>430</width>
			<height>450</height>
			<texture>XBacmenu.png</texture>
		</control>
  		<control type="label" id="82">
        		<description>Selected Version Label</description>
        		<posx>320</posx>
        		<posy>430</posy>
			<label>-</label>
        		<width>300</width>
        		<font>font13</font>
			<textcolor>FFB2D4F5</textcolor>
			<visible>!Control.HasFocus(50)</visible>
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
  		</control>
  		<control type="label" id="83">
        		<description>Info Label</description>
        		<posx>290</posx>
        		<posy>130</posy>
			<label>-</label>
        		<width>300</width>
        		<font>font13</font>
			<textcolor>FFB2D4F5</textcolor>
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
  		</control>
        	<control type="textbox" id="72">
            		<description>msgbody</description>
            		<posx>175</posx>
            		<posy>150</posy>
            		<width>370</width>
            		<height>280</height>
            		<spinposx>175</spinposx>
            		<spinposy>320</spinposy>
            		<textureup>XBscroll-up.png</textureup>
            		<texturedown>XBscroll-down.png</texturedown>
            		<textureupfocus>XBscroll-up-focus.png</textureupfocus>
            		<texturedownfocus>XBscroll-down-focus.png</texturedownfocus>
            		<font>font13</font>
            		<textcolor>FFFFFFFF</textcolor>
            		<onup>72</onup> 
            		<ondown>72</ondown>
            		<onleft>61</onleft>
           		<onright>62</onright>
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
        	</control>
		<control type="group" id="9000">
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
			<posx>183</posx>
			<posy>462</posy>
			<control type="button" id="61">
				<description>Change Log Button</description>
				<posx>0</posx>
				<posy>0</posy>
      				<textoffsetx>2</textoffsetx>
      				<textoffsety></textoffsety>
				<label></label>
				<onup>50</onup>
				<onright>72</onright>
				<ondown>61</ondown>
				<onleft>61</onleft>
				<align>center</align>
				<aligny>center</aligny>
      				<texturefocus>XBbuttonF.png</texturefocus>
				<texturenofocus>XBbuttonNF.png</texturenofocus>
				<disabledcolor>60ffffff</disabledcolor>
				<width>97</width>
				<height>42</height>
			</control>
			<control type="button" id="62">
				<description>Update Button</description>
				<posx>258</posx>
				<posy>0</posy>
      				<textoffsetx>2</textoffsetx>
      				<textoffsety></textoffsety>
				<label></label>
				<onup>50</onup>
				<onright>63</onright>
				<ondown>62</ondown>
				<onleft>72 </onleft>
				<align>center</align>
				<aligny>center</aligny>
      				<texturefocus>XBbuttonF.png</texturefocus>
				<texturenofocus>XBbuttonNF.png</texturenofocus>
				<disabledcolor>60ffffff</disabledcolor>
				<width>97</width>
				<height>42</height>
			</control>
		</control>
		<control type="list" id="50">
			<posx>175</posx>
			<posy>155</posy>
			<width>400</width>
			<height>255</height>
			<onup>50</onup>
			<ondown>50</ondown>
			<scrolltime>200</scrolltime>
			<animation effect="fade" time="1000">WindowOpen</animation>
			<animation effect="fade" time="1000">WindowClose</animation>
			<itemlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>190</posx>
					<posy>-7</posy>
					<align>right</align>
					<aligny>center</aligny>
					<width>50</width>
					<height>40</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>2</posy>
					<width>390</width>
					<height>22</height>
					<font>font13</font>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>360</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>370</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(50)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="image">
					<width>370</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>!Control.HasFocus(50)</visible>
					<colordiffuse>FFFFFF00</colordiffuse>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="image">
					<posx>190</posx>
					<posy>-7</posy>
					<align>right</align>
					<aligny>center</aligny>
					<width>50</width>
					<height>40</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>2</posy>
					<width>390</width>
					<height>22</height>
					<font>font13</font>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>360</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>
	</controls>
</window>