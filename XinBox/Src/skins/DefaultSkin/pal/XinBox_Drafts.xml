<window>
	<defaultcontrol>50</defaultcontrol>
	<controls>
		<control type="image" id="60">
			<description>Drafts Bckground</description>
			<posx>-40</posx>
			<posy>0</posy>
			<width>650</width>
			<height>600</height>
			<texture>XBdrafts.png</texture>
			<animation effect="slide" start="-700,0" time="800">Visible</animation>
			<animation effect="slide" end="-700,0" time="800">Hidden</animation>
			<visible>Skin.HasSetting(draftlist)</visible>
		</control>
		<control type="list" id="50">
			<animation effect="slide" start="-700,0" time="800">Visible</animation>
			<animation effect="slide" end="-700,0" time="800">Hidden</animation>
			<visible>Skin.HasSetting(draftlist)</visible>
			<posx>125</posx>
			<posy>80</posy>
			<width>700</width>
			<height>310</height>
			<onleft>99</onleft>
			<onright>99</onright>
			<onup>99</onup>
			<ondown>99</ondown>
			<scrolltime>200</scrolltime>
			<itemlayout height="27">
				<control type="label">
					<posx>35</posx>
					<posy>0</posy>
					<width>332</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>35</posx>
					<posy>25</posy>
					<width>332</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>578</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(50)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="label">
					<posx>35</posx>
					<posy>0</posy>
					<width>332</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>35</posx>
					<posy>25</posy>
					<width>332</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>
    </controls>

</window>
