<window>
	<defaultcontrol>63</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="image" id="65">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
			<texture>XBbackground.png</texture>
		</control>

        	<control type="label" id="66">
			<description>title</description>
			<posx>250</posx>
			<posy>70</posy>
			<width>200</width>
			<label>XinBox</label>
			<height>100</height>
			<align>left</align>
			<font>special13</font>
			<textcolor>FFB2D4F5</textcolor>
		</control>
        	<control type="label" id="70">
			<description>xinbox empty</description>
			<posx>300</posx>
			<posy>100</posy>
			<width>200</width>
			<height>100</height>
			<align>left</align>
			<font>font18</font>
			<textcolor>FFB2D4F5</textcolor>
		</control>
		<control type="group" id="9000">
			<posx>20</posx>
			<posy>-20</posy>
			<control type="button" id="61">
				<description>XinBox1 button</description>
				<posx>0</posx>
				<posy>120</posy>
				<label>XinBox 1</label>
				<onleft>61</onleft>
				<onright>50</onright>
				<onup>64</onup>
				<ondown>62</ondown>
				<disabledcolor>60ffffff</disabledcolor>
			</control>
			<control type="button" id="62">
				<description>XinBox2 button</description>
				<posx>0</posx>
				<posy>157</posy>
				<label>XinBox 2</label>
				<onleft>62</onleft>
				<onright>50</onright>
				<onup>61</onup>
				<ondown>63</ondown>
				<disabledcolor>60ffffff</disabledcolor>
			</control>
			<control type="button" id="63">
				<description>Settings Button</description>
				<posx>0</posx>
				<posy>194</posy>
				<onleft>65</onleft>
				<onright>50</onright>
				<onup>62</onup>
				<ondown>64</ondown>
			</control>
			<control type="button" id="64">
				<description>Minimode Button</description>
				<posx>0</posx>
				<posy>231</posy>
				<onleft>66</onleft>
				<onright>50</onright>
				<onup>63</onup>
				<ondown>61</ondown>
				<disabledcolor>60ffffff</disabledcolor>
			</control>
		</control>
		<control type="list" id="50">
			<posx>211</posx>
			<posy>125</posy>
			<width>455</width>
			<height>200</height>
			<onleft>9000</onleft>
			<onright>69</onright>
			<onup>50</onup>
			<ondown>50</ondown>
			<viewtype label="535">list</viewtype>
			<pagecontrol>69</pagecontrol>
			<scrolltime>200</scrolltime>
			<visible>Skin.HasSetting(xblistnotempty)</visible>
			<itemlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture>XBlist-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>5</posx>
					<posy>3</posy>
					<width>20</width>
					<height>20</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>2</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>430</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="27">
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>!Control.HasFocus(50)</visible>
					<colordiffuse>FF000000</colordiffuse>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="image">
					<width>440</width>
					<height>25</height>
					<posx>0</posx>
					<posy>0</posy>
					<visible>Control.HasFocus(50)</visible>
					<texture>XBlist-focus.png</texture>
				</control>
				<control type="image">
					<posx>5</posx>
					<posy>3</posy>
					<width>20</width>
					<height>20</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>2</posy>
					<width>380</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>430</posx>
					<posy>2</posy>
					<width>300</width>
					<height>22</height>
					<font>font13</font>
					<selectedcolor>FFe2ff43</selectedcolor>
					<textcolor>60ffffff</textcolor>
					<align>right</align>
					<info>ListItem.Label2</info>
				</control>
			</focusedlayout>
		</control>
        	<control type="textbox" id="67">
            		<description>email preview</description>
            		<posx>216</posx>
            		<posy>340</posy>
            		<width>456</width>
            		<height>200</height>
            		<textureup>XBscroll-up.png</textureup>
            		<texturedown>XBscroll-down.png</texturedown>
            		<textureupfocus>XBscroll-up-focus.gif</textureupfocus>
            		<texturedownfocus>XBscroll-down-focus.gif</texturedownfocus>
            		<font>font13</font>
            		<textcolor>FFFFFFFF</textcolor>
            		<onup>67</onup>
            		<ondown>67</ondown>
            		<onleft>67</onleft>
            		<onright>67</onright>
        	</control>
		<control type="scrollbar" id="69">
			<posx>680</posx>
			<posy>100</posy>
			<width>17</width>
			<height>200</height>
			<texturesliderbackground border="4">XBscrollbar_background.png</texturesliderbackground>
			<texturesliderbar border="3">XBscrollbar_bar.png</texturesliderbar>
			<texturesliderbarfocus border="3">XBscrollbar_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>XBscrollbar_nib.png</textureslidernib>
			<textureslidernibfocus>XBscrollbar_nib_focus.png</textureslidernibfocus>
			<onleft>50</onleft>
			<onright>9000</onright>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<visible>Control.IsVisible(50)</visible>
			<visible>!Skin.HasSetting(UseScrollbars) + !Control.IsVisible(56)</visible>
			<visible>![Window.IsActive(myvideolibrary) + Control.IsVisible(51)]</visible>
		</control>

    </controls>

</window>
